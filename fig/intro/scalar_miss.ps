(plotlib/plotfonts.ps) run
(plotlib/plotmisc.ps) run
(plotlib/plotparameters.ps) run
%%EndProcSet

/width {20 } bind def
/height {3} bind def
/xmax {1} bind def
/xmin {-1} bind def
/ymax {1} bind def
/ymin {-1} bind def
/linesize {2} bind def

(plotlib/plotutilities.ps) run
(plotlib/casimirutilities.ps) run
0 setgray 
startplot


gsave
fillcol
1.5 in 3 in moveto
0 -5 in rlineto
1.2 in 0 rlineto
0 5 in rlineto
 -1.2 in 0  rlineto
fill
grestore 



gsave
0 in 1.7 in translate
mark
1 0.75 scale
(intro/data/cp1.loop) run % load loop 1
TMcol
NoPlotClip
10000 PlotStackData
% plot data points
mark

grestore


gsave
0 setgray
1.5 in 3 in moveto
0 in -5 in rlineto
stroke
grestore



%atom
gsave
-120 50 5 atom
grestore


endplotnoborder
