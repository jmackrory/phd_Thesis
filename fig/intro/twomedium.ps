%!PS-Adobe-2.0 
%%EndComments
%%BeginProlog
(plotlib/plotfonts.ps) run
(plotlib/plotparameters.ps) run
(plotlib/plotarrows.ps) run
/width {8 } bind def
/height {11} bind def
/xmax {3} def
/xmin {-10} def
/ymin {0} def
/ymax {2} def
/sigma {3} bind def
%size of ellipse
/ComputerModern-Regular findfont 14 scalefont setfont
/titlefont {/ComputerModernExtraBold} def
/titlesize {30}         bind def % size of plot title
/subsize {20}         bind def % size of plot title
/cas_xoffset {-6}  def
/cas_yoffset {.25}  def
(plotlib/plotutilities.ps) run
(plotlib/casimirutilities.ps) run

1 in 5 in  translate

%TwoSlab
%Wall
gsave
4 in 0 translate
0 setgray
2 setlinewidth

newpath
0 in 2 in moveto
0 in -4 in rlineto
stroke

newpath
fillcol
0 in 2 in moveto
0 in -4 in rlineto
1.5 in 0 in rlineto
0 in 4 in rlineto
-1.5 in 0 in rlineto
fill
grestore

%Wall 2
gsave
1 in 0 translate
0 setgray
2 setlinewidth

newpath
0 in 2 in moveto
0 in -4 in rlineto
stroke

newpath
fillcol
0 in 2 in  moveto
0 in -4 in rlineto
-1.5 in 0 in rlineto
0 in 4 in rlineto
1.5 in 0 in rlineto
fill
grestore



%Force arrows
gsave
0 setgray
1 in 1 in  2 in 1 in 2 1 arrowend
4 in 1 in  3 in 1 in 2 1 arrowend

1 in -1.5 in  4 in -1.5 in 1.5 1 arrowboth
%Labels
2.5 in 0.9 in  moveto
[  /KahoeTechLatin normal (Eo(d)) ]
		titlesize PrintArrayCenter

2.5 in -1.3 in  moveto[
  /KahoeTechLatin normal (d) ]
		titlesize PrintArrayCenter


0.15 in 0.0 in moveto
[  /KahoeTechGreek normal (c) ]
		titlesize PrintArrayCenter
[  /KahoeTechGreek subscript (1) ]
		subsize PrintArrayCenter

	4.75 in 0.0 		 in moveto
[  /KahoeTechGreek normal (c) ]
		titlesize PrintArrayCenter
[  /KahoeTechGreek subscript (2) ]
		subsize PrintArrayCenter

		
grestore

%Labels




