%!PS-Adobe-2.0 
%%EndComments
%%BeginProlog
(plotlib/plotfonts.ps) run
(plotlib/plotparameters.ps) run
(plotlib/plotarrows.ps) run
%Height and width in inches
% In plotutilities these are converted to real numbers
/xmaxstr (3.2) def
/xminstr (-3.2) def
/ymaxstr (0.2) def
/yminstr (-3.5) def

%load utilities
twocolumndimensions
(plotlib/plotutilities.ps) run

/TEcol{0 0.67 0 setrgbcolor} def 
/TMcol{0 0 1 setrgbcolor} def

%/hitcol{.6 0. 0.7 setrgbcolor} def
/outcol{0 0 0 setrgbcolor} def
/hitcol{0.0 0.67 0 setrgbcolor} def
/ytitleoffset {-0.3 in}   def  % horizontal offset of y-axis title

startplot
%Energy curve 
1 setlinewidth
/symbolsz 3 def % in points
outcol
(data/TM_atom_wall_anlt.clean.dat) run % load loop 1
20 PlotStackDataLogLog

TMcol
(data/TM_atom_wall_eff.clean.dat) run % load loop 1
20 PlotStackDataPointsYerrLogLog

%XAxisLabelsLog
%YAxisLabelsLog
[ (-3) (-2) (-1) (0) ] ExtraYTicksLog
[ (-3) (-2) (-1) (0) (1) (2) (3) ] ExtraXTicksLog
[ /KahoeTechGreek normal (c)]  XAxisTitle
[ /KahoeTechGreek normal (h o   (c))]  YAxisTitle
[ /ComputerModern-Regular subscript (TM   )]  YAxisTitle

%start log-linear inset

/insetoffx {.65 in} def
/insetoffy {-0.15 in} def

/ytitlevertnudge {-0.1 in} def
SetLogAxisX NoSetLogAxisY

/xmaxstr (3.2) def
/xminstr (-3.2) def
/ymaxstr (1) def
/yminstr (0) def

startinset

gsave
%Energy curve 
1 setlinewidth
outcol
(data/TM_atom_wall_anlt.clean.dat) run % load loop 1
20 PlotStackData

TMcol
(data/TM_atom_wall_eff.clean.dat) run % load loop 1
20 PlotStackDataPointsYerr

grestore

%XAxisLabelsLog
%YAxisLabels
[ (0) (0.5) (1.0) ] ExtraYTicks
%[ (0.05) (0.1) ] ExtraYTicks
[ (-2) (0) (2) ] ExtraXTicksLog
[ /KahoeTechGreek normal (c)]  XAxisTitle
[ /KahoeTechGreek normal (h o   (c))]  YAxisTitle
[ /ComputerModern-Regular subscript (TM   )]  YAxisTitle

endinset


endplot
